// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "#src/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "./src/**/*.ts", "./src/**/*.tsx", "src/Pages/_app.js", "src/Pages/index.js", "src/models/MyModel/test.js", "src/Comps/layout/App/test.js", "src/Comps/misc/spinner/test.js", "src/Comps/misc/test.js", "src/Comps/Navigation/Footer/test.js", "src/Comps/Navigation/Header/test.js"],
  "exclude": ["node_modules", "creditor", "test"]
}
